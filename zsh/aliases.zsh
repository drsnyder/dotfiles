alias reload!='. ~/.zshrc'

alias cls='clear' # Good 'ol Clear Screen command
alias notes='vim `fzf --preview="cat {}" --preview-window=right:70%:wrap`'
alias ag='ag --hidden'
alias mux=tmuxinator

alias ls='lsd'
alias l='ls -l'
alias la='ls -a'
alias lla='ls -la'
alias lt='ls --tree'
alias lrt='ls -alrth'

